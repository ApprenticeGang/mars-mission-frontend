{
  "name": "mars-mission-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.57",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "4.1.0",
    "@typescript-eslint/parser": "4.1.0",
    "d3-scale": "^3.2.2",
    "es6-tween": "^5.5.11",
    "eslint": "6.6.0",
    "eslint-plugin-react": "7.20.6",
    "node-sass": "4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-globe": "^5.0.2",
    "react-router": "5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "resize-observer-polyfill": "^1.5.1",
    "typescript": "^3.7.5",
    "typescript-plugin-css-modules": "2.4.0",
    "three": "^0.120.1",
    "three-glow-mesh": "^0.1.2",
    "three.interaction": "^0.2.3",
    "tippy.js": "^6.2.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test:coverage": "react-scripts test --watchAll=false --coverage",
    "test:watch": "react-scripts test --watchAll",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "lint:fix": "eslint --fix src/**/*.ts src/**/*.tsx",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.tsx",
      "!src/react-app-env.d.ts",
      "!src/InteractiveMars/Markers.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
